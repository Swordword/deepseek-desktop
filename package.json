    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^34.3.0",
    "electron-builder": "^24.13.3",
    "next": "^15.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0"
  }
