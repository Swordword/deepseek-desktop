{
  "name": "deepseek-desktop",
  "version": "1.0.0",
  "description": "DeepSeek Desktop Client",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build && tsc --project tsconfig.electron.json",
